doccument-YEarly_bonus.docum
- sudo apt install python3 python3-pip -y
- python3 -m venv ~/oletools-env
- source ~/oletools-env/bin/activate
- pip install oletools
- oleid YearlyBonus.docm
- olevba YearlyBonus.docm
- olevba -c YearlyBonus.docm >extr_vba.txt
- cat extr_vba.txt

